<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:grid="urn:import:com.google.gwt.user.cellview.client">

	<ui:style>
		.gimmeSomeRoom {
			padding: 15px 25px;
		}
	</ui:style>

	<g:SimplePanel addStyleNames="{style.gimmeSomeRoom}">
		<g:ResizeLayoutPanel width="100%" height="100%">
			<grid:DataGrid ui:field="grid" />
		</g:ResizeLayoutPanel>
	</g:SimplePanel>

</ui:UiBinder> 