<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client">

	<ui:with field="resource"
		type="org.geomajas.puregwt.client.resource.GeomajasWidgetResource" />

	<g:AbsolutePanel addStyleNames="{resource.css.touchZoomWidget}">
		<g:Button ui:field="zoomOut" styleName="{resource.css.touchZoomOutBut}"
			addStyleNames="{resource.css.touchZoomButNormal}">-</g:Button>
		<g:Button ui:field="zoomIn" styleName="{resource.css.touchZoomInBut}"
			addStyleNames="{resource.css.touchZoomButNormal}">+</g:Button>
	</g:AbsolutePanel>
</ui:UiBinder> 